# Nextcloud Environment Variables
# Copy this file to .env and fill in the values
#
# Generate passwords with: openssl rand -base64 32

# Database
DB_ROOT_PASSWORD=<generate-with-openssl-rand>
DB_PASSWORD=<generate-with-openssl-rand>

# Nextcloud Admin
ADMIN_USER=admin
ADMIN_PASSWORD=<generate-with-openssl-rand>

# Whiteboard (for real-time collaboration)
WHITEBOARD_SECRET=<generate-with-openssl-rand>
